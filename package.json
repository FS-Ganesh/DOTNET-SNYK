{
  "name": "dotnet-deps-parser",
  "description": "Generate a dep tree given a collection of manifests",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && npm run test:tap && npm run test:jest",
    "test:tap": "npm run build && tap test/lib/**/*.test.ts -Rspec --timeout=300",
    "test:jest": "jest",
    "format:check": "prettier --check '{test,src}/**/*.{js,ts}' && tslint -p tsconfig.json",
    "format": "prettier --write '{test,src}/**/*.{js,ts}' && tslint -p tsconfig.json --fix",
    "pre-push": "npm run format:check",
    "lint": "npm run format:check && eslint --cache '{test,src}/**/*.{js,ts}'",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/dotnet-deps-parser.git"
  },
  "author": "snyk.io",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk/dotnet-deps-parser#readme",
  "dependencies": {
    "lodash.isempty": "^4.4.0",
    "lodash.set": "^4.3.2",
    "lodash.uniq": "^4.5.0",
    "source-map-support": "^0.5.7",
    "tslib": "^1.10.0",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@types/node": "^10",
    "@types/xml2js": "0.4.5",
    "@types/jest": "^23.3.2",
    "jest": "23.6.0",
    "ts-jest": "^23.10.1",
    "tap": "^14.10.7",
    "tslint": "5.11.0",
    "typescript": "3.7.3"
  }
}
